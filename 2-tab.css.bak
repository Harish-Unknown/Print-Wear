*{
	margin:0;
	padding:0;
	font-family:poppins,sans-serif;
    text-decoration:none;
}



div.banner{
	background-color:#fff;
	padding:40px;
}

div.nadu{
	font-size:12px;
	margin: 30px 0px 30px 300px;
}

div.icon{
	display:flex;
	float:right;
}

i.fa{
    margin:0px 10px 0px 10px;
}



div.large{
	margin:20px 180px 40px 300px;
	padding:30px;
	border:2px solid transparent;
	border-radius:10px;
	width:60%;
	min-width:900px;
	background-color:white;
	box-shadow:0px 2px 5px #666666;
}

div.chemical{
	float:right;
	margin-top:-20px;
	border:2px solid red transparent;
	clear:right;
}


.chemical a{
	color:#ED2939;
	cursor:pointer;
	border:1px solid #ED2939;
	padding:12px 34px;
	background:transparent;
	text-decoration:none;
}

.chemical a:hover{
	border:1px solid #f44336;
	color:#fff;
	background:#f44336;
	transition:0.3s;
}



div.mue{
	float:right;
	margin-top:-20px;
	text-decoration:underline;
	text-decoration-color:red;
	margin-left:30px;
	padding: 0px 250px 0px 0px;
}

p.fly{
	float:right;
	margin-top:15px;
}

div.nin{
	margin:90px 60px 20px 60px;
	padding:10px 60px 10px 170px;
	width:100%;
}
#chop2{
	position:relative;
	left:750px;
	top:25px;
}
#chop1{
	float:left;
	margin:20px;
	padding-left:30px;
	
}


div.blas{
	position:absolute;
	left:200px;
    
}

div.keepin{
	margin-top:10px;
	margin-right:60px;
	margin-bottom:20px;
	margin-left:60px;
	padding:10px 60px 60px 170px;
}
#column1,#column2,#column3{
	width:200px;
	border:2px solid transparent;
	border-radius:10px;
	height:95px;
	margin:30px;
	padding:20px 0px 0px 20px;
	background-color:#fff;
	box-shadow:0px 2px 5px #666666;
}


div.biggie{
	position:absolute;
	height:350px;
	border:2px solid transparent;
	border-radius:10px;
	top:950px;
	left:450px;
	margin:60px;
	width:770px;
	padding:40px;
	background-color:white;
	box-shadow:0px 2px 5px #666666;
}



p.niko{
	font-size: 1rem;
	position:relative;
	left:10px;
}

p.jar{
	font-size: 1rem;
	position:relative;
	left:10px;
}

p.niko:after{
	content: " ";
	position:absolute;
	bottom: 0px;
	left: 50px;
	right: 0;
	height: 0.5em;
	border-top: 1px solid black;
	padding:1px;
	margin:1px;
}



div.final{
	margin:20px 40px 40px 270px;
}

div.putin1{
	margin:20px 180px 40px 250px;
	display:flex;
}

div.putin{
	border:2px solid transparent;
	width:400px;
	border-radius:8px;
	height:65px;
	padding:20px;
	margin:15px;
	background-color:#fff;
	box-shadow:0px 2px 5px #666666;
}





